{
  "name": "rig-class.js",
  "description": "Fast prototype based javascript classes for Node.js and the browser, ES6 compatible",
  "version": "1.2.3",
  "author": "Christopher Maples <cmaples77@gmail.com>",
  "licence": "LGPL-3.0",
  "engines": {
    "node": ">=v0.10.0"
  },
  "dependencies": {
    "bower": "^1.6.5",
    "grunt": "^0.4.5"
  },
  "devDependencies": {
    "connect": "^3.3.3",
    "grunt-bower-requirejs": "^1.1.1",
    "grunt-contrib-concat": "^0.5.0",
    "grunt-contrib-connect": "^0.9.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-jasmine": "^0.8.1",
    "grunt-contrib-uglify": "^0.6.0",
    "grunt-mocha": "^0.4.11",
    "grunt-mocha-test": "^0.12.4",
    "rig-grunt-npm-dev-tools": "git+ssh://git@dnvrco-vm-coed0018.conops.timewarnercable.com:7999/npms/grunt-npm-dev-tools.git#master",
    "grunt-template-jasmine-istanbul": "^0.3.0",
    "grunt-template-jasmine-requirejs": "^0.2.0",
    "mocha": "^2.0.1",
    "requirejs": "^2.1.15"
  },
  "optionalDependencies": {},
  "keywords": [
    "class",
    "es6",
    "node",
    "browser"
  ],
  "scripts": {
    "devlink": "npm link && npm link rig-grunt-npm-dev-tools",
    "devunlink": "npm unlink rig-grunt-npm-dev-tools && npm unlink",
    "test": "grunt test",
    "postinstall": "grunt project:extend && npm run git.install.hooks",
    "idPortProcess": "netstat -tulpn | grep 8010",
    "copy.nodeunit": "mkdir -p ./test && cp -R ./node_modules/rig-grunt-npm-dev-tools/lib/nodeunit/test/ ./test",
    "getversion": "node -e \"console.log(require('./package.json').version)\"",
    "git.reset.tags": "git tag -l | xargs git tag -d && git fetch --tags",
    "git.revert.commit": "git reset --soft HEAD~1",
    "git.install.hooks": "cp ./node_modules/rig-grunt-npm-dev-tools/githooks/* .git/hooks/"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  }
}
